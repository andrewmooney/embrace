{
	"fmt": {
		"options": {
			"lineWidth": 120,
			"useTabs": false,
			"indentWidth": 2,
			"singleQuote": true,
			"proseWrap": "preserve"
		}
	},
	"compilerOptions": {
		"lib": ["es2020", "dom", "dom.iterable"],
		"jsx": "react-jsx",
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
	}
}
